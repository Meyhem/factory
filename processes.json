    {
        "name": "sulfuric acid production",
        "stages": [
            {
                "name": "sulfur extraction or roasting",
                "stage": 1,
                "inputs": [
                    "sulfide ores or sulfur"
                ],
                "outputs": [
                    "sulfur dioxide"
                ]
            },
            {
                "name": "conversion",
                "stage": 2,
                "inputs": [
                    "sulfur dioxide",
                    "oxygen",
                    "vanadium pentoxide catalyst"
                ],
                "outputs": [
                    "sulfur trioxide"
                ]
            },
            {
                "name": "absorption",
                "stage": 3,
                "inputs": [
                    "sulfur trioxide",
                    "concentrated sulfuric acid"
                ],
                "outputs": [
                    "oleum"
                ]
            },
            {
                "name": "dilution",
                "stage": 4,
                "inputs": [
                    "oleum",
                    "water"
                ],
                "outputs": [
                    "sulfuric acid"
                ]
            }
        ]
    },
    {
        "name": "ammonium nitrate fertilizer production",
        "stages": [
            {
                "name": "ammonia synthesis",
                "stage": 1,
                "inputs": [
                    "nitrogen",
                    "hydrogen"
                ],
                "outputs": [
                    "ammonia"
                ]
            },
            {
                "name": "nitric acid production",
                "stage": 2,
                "inputs": [
                    "ammonia",
                    "oxygen",
                    "platinum catalyst"
                ],
                "outputs": [
                    "nitric acid"
                ]
            },
            {
                "name": "neutralization",
                "stage": 3,
                "inputs": [
                    "ammonia",
                    "nitric acid"
                ],
                "outputs": [
                    "ammonium nitrate solution"
                ]
            },
            {
                "name": "crystallization and drying",
                "stage": 4,
                "inputs": [
                    "ammonium nitrate solution"
                ],
                "outputs": [
                    "ammonium nitrate fertilizer"
                ]
            }
        ]
    },
    {
        "name": "aluminum from bauxite ore",
        "stages": [
            {
                "name": "bauxite mining",
                "stage": 1,
                "inputs": [],
                "outputs": [
                    "bauxite ore"
                ]
            },
            {
                "name": "bayer process - digestion",
                "stage": 2,
                "inputs": [
                    "bauxite ore",
                    "sodium hydroxide"
                ],
                "outputs": [
                    "aluminate solution"
                ]
            },
            {
                "name": "bayer process - precipitation",
                "stage": 3,
                "inputs": [
                    "aluminate solution"
                ],
                "outputs": [
                    "alumina hydrate"
                ]
            },
            {
                "name": "calcination",
                "stage": 4,
                "inputs": [
                    "alumina hydrate",
                    "heat"
                ],
                "outputs": [
                    "alumina"
                ]
            },
            {
                "name": "hall-heroult electrolysis",
                "stage": 5,
                "inputs": [
                    "alumina",
                    "cryolite",
                    "electricity"
                ],
                "outputs": [
                    "aluminum"
                ]
            }
        ]
    }
