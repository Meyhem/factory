    {
        "name": "cement manufacture",
        "stages": [
            {
                "name": "quarry",
                "stage": 1,
                "inputs": [],
                "outputs": [
                    "limestone",
                    "clay",
                    "other raw materials"
                ]
            },
            {
                "name": "milling",
                "stage": 2,
                "inputs": [
                    "limestone",
                    "clay",
                    "other raw materials"
                ],
                "outputs": [
                    "raw meal"
                ]
            },
            {
                "name": "kilning",
                "stage": 3,
                "inputs": [
                    "raw meal",
                    "heat"
                ],
                "outputs": [
                    "clinker"
                ]
            },
            {
                "name": "finish milling",
                "stage": 4,
                "inputs": [
